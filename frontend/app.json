{
  "expo": {
    "name": "Gestion de Tienda",
    "slug": "gestion-tienda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tienda.gestion"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#121212"
      },
      "package": "com.tienda.gestion"
    },
    "plugins": [
      "expo-sqlite",
      "expo-font"
    ]
  }
}
